# GaN

structure = wz

[lattice]
a = 0.3189
c = 0.5185


[valenceband]
#E_v = -2.64
# from Nextnano
E_v = -0.71033


[conductionband]

m_c_xx = 0.202
m_c_zz = 0.206



[bandgap]
# T = 0
Eg_G = 3.51

varshni_alpha_G = 0.909e-3
varshni_beta_G = 830


[kdotp]

A1 = -7.21
A2 = -0.44
A3 = 6.68
A4 = -3.46
A5 = -3.4
A6 = -4.9

delta_cr = 0.01
delta_s = 0.017


[deformation_potentials]
a_x = -6.80
a_z = -8.60

D1 = -3.7
D2 = 4.5
D3 = 8.2
D4 = -4.1
D5 = -4.0
D6 = -5.5


[permittivity]
# static dielectric constant
permittivity = 10.01

# Optical dielectric constant @ 3.5 eV
optical_epsilon_x = 7.3
optical_epsilon_z = 7.3 




# stiffness data Ambacher theory1 
[elasticity]
C11 = 367.0
C12 = 135.0
C13 = 103.0
C33 = 405.0
C44 = 95.0

# piezoelectricity data  
[piezoelectricity]
e33 = 0.67
e31 = -0.39
e15 = -0.15

[pyroelectricity]
Pz = -0.034



#
# mobility
#

# constant
[mobility/constant]
mu_max = (1500, 200.0)
exponent = (1.0, 2.1)


# doping dependent
[mobility/doping_dependent]
mobility_formula = 1
mumin1 = (85, 		33)
mumin2 = (75, 		0)
mu1    = (50, 		20)
Pc     = (6.5e15, 	5e15)
Cr     = (9.5e16, 	8e16)
Cs     = (7.2e19, 	8e20)
alpha  = (0.55, 	0.55)
beta   = (0.75, 	0.7)


# high field dependency
[mobility/field_dependent]
Vsat_Formula = 2
beta0    = (1.7, 	1.7)
A_vsat   = (1.9e+07, 	1.5000e+07)
B_vsat   = (0,		0)
vsat_min = (1.000e+07, 	1.000e+07)
betaexp  = (0,		0)


#
# recombination
#

# SRH recombination
[recombination/SRH]
Etrap  = 0.0
taumin = (0.0,		0.0)
taumax = (1.0e-9,	1.0e-9)
Nref   = (1.0e+16,	1.0e+16)
gamma  = (1,		1)
Talpha = (-1.5,		-1.5)
Tcoeff = (2.55,		2.55)


# direct
[recombination/direct]
C = 0.47e-10


[recombination/auger]
# R_Auger = ( C_n n + C_p p ) ( n p - ni_eff^2)
# with C_n,p = (A + B (T/T0) + C (T/T0)^2) (1 + H exp(-{n,p}/N0))
# The values below should be checked
A  = (1.0000e-34 ,	1.0000e-34)	# [cm^6/s]
B  = (0.0000e+00 ,	0.0000e+00)	# [cm^6/s]
C  = (0.0000e+00 ,	0.0000e+00)	# [cm^6/s]
H  = (0.0000e+00 ,	0.0000e+00)	# [1]
N0 = (1.0000e+18 ,	1.0000e+18)	# [cm^(-3)]



#Lattice thermal conductivity 
[thermal_conductivity/constant]
therm_lat_cond_x = 1.3
therm_lat_cond_z = 1.3



#Atomistic structure informations
#
[atomistic_structure]
lattice_type = hexagonal

#Basis 

#Number of basis specie
n_basis_specie = 2
#Number of basis atoms for any specie
n_1 = 2
n_2 = 2
#Basis vectors T_"specie"_"#of atom"_"direction"
T_1_1_x = 0.333333
T_1_1_y = 0.666666
T_1_1_z = 0.0
T_1_2_x = 0.666666
T_1_2_y = 0.333333
T_1_2_z = 0.5
T_2_1_x = 0.333333
T_2_1_y = 0.666666
T_2_1_z = 0.375000
T_2_2_x = 0.666666
T_2_2_y = 0.333333
T_2_2_z = 0.875000

#Name of species
specie_1 = Ga
specie_2 = N

[densityfunctional_tb]

dataset = nitrides

