Device qdot
{

  meshfile = dot5.msh
  mesh_units = 1e-9
  dimension = 3

  y-growth-direction = (-1,0,1,0)
  x-growth-direction = (-1,2,-1,0)   
  z-growth-direction = (0,0,0,1)

  Region dot
  {
    mesh_regions = 1
    material = GaN
    x = 0.0

   
   Doping { 
    Nd = 1e15
    type = acceptor
    Ed = 0.025
  }

  }

  Region  buffer
  {
    mesh_regions = 2
    material = AlGaN
    x = 0.3
   

   Doping { 
    Nd = 1e15
    type = acceptor
    Ed = 0.025
   }
    
   
  }

  Region  buffer1
  {
    mesh_regions = 3
    material = AlGaN
    x = 0.3

   Doping { 
    Nd = 1e15
    type = acceptor
    Ed = 0.025
   }
    
   
  }
  
  Region  buffer2
  {
    mesh_regions = 4
    material = AlGaN
    x = 0.3


   Doping {
    Nd = 1e15
    type = acceptor
    Ed = 0.025
   }
   
  }
}

Module elasticity 
{

  name = str
  plot = (Strain, Stress, Displacement)

  Physics 
  {
    body_force lattice_mismatch 
    {
      reference_material = AlGaN

      x = 0.3

      y-growth-direction = (-1,0,1,0)
      x-growth-direction = (-1,2,-1,0)   
      z-growth-direction = (0,0,0,1)
    }
       
  }

  Contact substrate  
  {
  regions =  1
  type = clamp}

}



