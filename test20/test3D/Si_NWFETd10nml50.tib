
# Description of  the  device physical regions
Device
{
  meshfile = Si_NWFETd10nml50_ox.msh  
  mesh_units = 1e-9

  material = Si
  
  #x-growth-direction = (1, 1, 0)
  #y-growth-direction = (1, -1, 0)
  #z-growth-direction = (0, 0, 1)

   Region source_region
   {
     Doping {
       type = donor	
       Nd = 1e20 
     }
   }

   Region drain_region
   {
     Doping {
       type = donor #acceptor
       Nd = 1e20 
     }
   }

   Region ext_source_region
   {
     Doping {
       type = donor #acceptor
       Nd = 1e20 
     }
   }

  Region ext_drain_region
   {
     Doping {
       type = donor #acceptor
       Nd = 1e20 
     }
   }

   Region oxide
   {
     material = SiO2
   }
   
   Region oxide1
   {
     material = SiO2
   }
  
   Region oxide2
   {
     material = SiO2
   }

   Region channel1
   {
     Doping {
       type = donor #acceptor
       Nd = 1e20
     } 
   }

   Region channel4
   {
     Doping {
       type = donor #acceptor
       Nd = 1e20 
     }   
   }


   Region channel6
   {
     Doping {
       type = donor #acceptor
       Nd = 1e20 
     }
   }

   Cluster channel
   {
    regions=(channel1, channel4, channel6)
   }
   
   Quantum_contact source 
   {
    regions = (source_region,oxide2) 
    length = 0.5
   }

   Quantum_contact drain 
   {
    regions = (drain_region,oxide1) 
    length = 0.5
   }

}


# Definition of  Simulation Models  and associated Boundary Conditions 
Module driftdiffusion
{ 
  regions = (source_region,drain_region,ext_source_region,ext_drain_region,channel,oxide,oxide1,oxide2)
  # is default
  # statistics = fermidirac

  # we solve only for electrons
  coupling = electrons

  #save_state = true
  load_state = outputd10nml50_ox/driftdiffusion.tsv  #_Vd_0.3_Vg_0.4.tsv
  solve_after_load = true

  plot = (Ec, Ev, eQFermi, eQFermi, eDensity, hDensity,
          eCurrentDensity, hCurrentDensity, NetRecombination,
          ElField, ElPotential, ContactCurrent)

  Physics
  {
    #recombination
    #{
    #  model = srh
    #}

    electron_mobility field_dependent
    {
      regions = (channel,source_region,drain_region)
      low_field_model = doping_dependent
    }

    electron_mobility constant 
    {
      regions = (oxide,oxide1,oxide2)
    }
    
    conduction_band
    {
      density_of_states bulk_kp
      {
        regions = all
      }
    }

    valence_band
    {
      density_of_states bulk_kp
      {
        regions = all 
      }
    }

  
 

  }


  #Contact gate
  #{
   # type = schottky
   # barrier_height = 3.39

   # voltage = $Vg[0.0]
  #}

  Contact ext_source
  {
    voltage = 0.0
  }
 
  Contact ext_drain
  {
    voltage = $Vd[0.0]
  }
}

Module efaschroedinger
{
  name = quantum_el


  regions = (channel,source_region,drain_region,oxide,oxide1,oxide2)
  
  particle = el
  poisson_model_name = driftdiffusion

  plot = (EigenFunctions, EigenEnergy, EnergyLevels,ProbabilityDensity,QuantumDensity)

  Solver
  {
    number_of_eigenstates = 60 
  }	


  Physics
  {
    model =conduction_band
    minimum_name = X
    valley = 3
    #model = kp   #  k.p  for  valence  band
    #kp_model = 8x8
  }

}



