
# Description of  the  device physical regions
Device
{
  meshfile = Si_NWFETd10nml50.msh  
  mesh_units = 1e-9

  material = Si
  
  #x-growth-direction = (1, 1, 0)
  #y-growth-direction = (1, -1, 0)
  #z-growth-direction = (0, 0, 1)

   Region source_region
   {
     Doping {
       type = acceptor			
       Nd = 2e20 
     }
   }

   Region drain_region
   {
     Doping {
       type = donor #acceptor
       Nd = 2e20 
     }
   }

   Region ext_source_region
   {
     Doping {
       type = donor #acceptor
       Nd = 2e20 
     }
   }

  Region ext_drain_region
   {
     Doping {
       type = donor #acceptor
       Nd = 2e20 
     }
   }

   Region oxide
   {
     material = SiO2
   }


   Region channel1
   {
   }

   Region channel4
   {
   }

   Region channel6
   {
   }

   Cluster channel
   {
    regions=(channel1, channel4, channel6)
   }
   
   Quantum_contact source #inside_s
   {
    regions = source_region  #channel4
    length = 0.5
   }

   Quantum_contact drain #inside_d
   {
    regions = drain_region #channel6
    length = 0.5
   }

}


# Definition of  Simulation Models  and associated Boundary Conditions 
Module driftdiffusion
{ 
  regions = (source_region,drain_region,ext_source_region,ext_drain_region,channel,oxide)
  # is default
  # statistics = fermidirac

  # we solve only for electrons
  coupling = electrons

  #save_state = true
  load_state = outputd10nml50/driftdiffusion_Vd_0.3_Vg_0.4.tsv
  #solve_after_load = true

  plot = (Ec, Ev, eQFermi, eQFermi, eDensity, hDensity,
          eCurrentDensity, hCurrentDensity, NetRecombination,
          ElField, ElPotential, ContactCurrent)

  Physics
  {
    #recombination
    #{
    #  model = srh
    #}

    electron_mobility field_dependent
    {
      low_field_model = doping_dependent
    }
  }


  Contact gate
  {
    type = schottky
    barrier_height = 3.39

    voltage = $Vg[0.0]
  }

  Contact ext_source
  {
    voltage = 0.0
  }
 
  Contact ext_drain
  {
    voltage = $Vd[0.0]
  }
}



Module sweep
{
  name = sweep_drain
  solve = driftdiffusion
  variable = $Vd
  start = 0
  stop = 0.3
  steps = 20 
  plot_data = true
}

Module sweep
{
  name = sweep_gate
  variable = $Vg
  start = 0
  stop = 0.4
  steps =  4 
  solve = sweep_drain 
}

Module sweep
{
  name = transchar
  variable = $Vg
  start = 0
  stop = 1
  steps =  50 
  plot_data = true
}

Module negf
{
  regions = (channel,source_region,drain_region)
  plot = (ReorderPotential, eDensity, Current)

  potential_simulation = driftdiffusion

  Boundary source{}
  Boundary drain{}
  
  Solver
  {
     
    Emin = -0.25
    Emax =  0.00 
    Estep = 0.01
  }

  Physics
  {
     #hamiltonian single_band {valley = X3} 
     #hamiltonian single_band {
     #     model = valence  
     #     band = HH }
 
     hamiltonian single_band {valley = X1}
     #hamiltonian single_band {valley = X2}
     #hamiltonian single_band {valley = X3}

  }
  
}


# Definition of  model-independent parameters of the Simulation
Simulation
{
  temperature = 300
  solve = (driftdiffusion, negf)
  resultpath = outputd10nml50
  scratchpath = /work/fpalomba
  output_format = vtk
}














