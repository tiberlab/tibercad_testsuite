Module negf
{
  regions = (channel,source_region,drain_region,oxide,oxide1,oxide2)
  plot = (elDensity, Current)

  potential_simulation = driftdiffusion

  Boundary source{}
  Boundary drain{}
  
  Solver
  {
    Emin = -0.25
    Emax =  0.00 
    Estep = 0.01
    Np_contour = (20,40)
  }

  Physics
  {
     #hamiltonian single_band {valley = X3} 
     #hamiltonian single_band {
     #     model = valence  
     #     band = HH }
 
     hamiltonian single_band {valley = X3}
     #hamiltonian single_band {valley = X2}
     #hamiltonian single_band {valley = X3}

  }
  
}


# Definition of  model-independent parameters of the Simulation
Simulation
{
  temperature = 300
  solve = (driftdiffusion,negf)
  searchpath = .
  resultpath = outputd10nml50_ox
  scratchpath = /work/fsantoni
  output_format = vtk
}














